[VOLUME]
command=echo "Volume: $(pactl list sinks | grep Volume | head -n1 | awk '{print $5}')"
interval=1

[BATTERY]
command=echo "Battery: $(acpi -b | grep -E -o '[0-9][0-9][0-9]?%')"
interval=1

[DISK]
command=df -h / | awk '/\//{ printf(" ğŸ’¾ %4s/%s \n", $4, $2) }'
interval=5

[MEM]
command=free -h | awk '/Mem:/ { printf(" ğŸ %5s/%s \n", $3, $2) }'
interval=2

#[WARP]
#command=warp-cli status
#interval=5

[USER]
command=whoami
interval=200

[TIME_DATE]
command=date +" %a, %d %b - %H:%M:%S"
interval=1
